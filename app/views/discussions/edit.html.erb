<h1>Editing discussion</h1>

<% form_for [@project,@discussion] do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', project_discussion_path %> |
<%= link_to 'Back', project_discussions_path %>